  <category label="30825"><!-- Tokai -->
    <setting label="30811" id="commschedule" type="bool" default="false"/><!-- 番組表取得 -->
    <setting type="sep"/>
{comm6}
  </category>
  <category label="30826"><!-- Kinki -->
    <setting label="30811" id="commschedule" type="bool" default="false"/><!-- 番組表取得 -->
    <setting type="sep"/>
{comm7}
  </category>
  <category label="30827"><!-- Chugoku/Shikoku -->
